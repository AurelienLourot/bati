{
  "name": "bati",
  "private": true,
  "version": "0.0.17",
  "description": "@batijs monorepo",
  "scripts": {
    "release": "pnpm -r prerelease && bumpp -r --commit --push --tag && pnpm run publish",
    "publish": "pnpm -r build && pnpm -r publish",
    "watch": "pnpm -r --parallel build --watch",
    "build": "pnpm -r build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "pnpm": {
    "overrides": {
      "recast": "npm:@putout/recast@1.12.1"
    }
  }
}
