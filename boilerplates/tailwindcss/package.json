{
  "name": "@batijs/tailwindcss",
  "private": true,
  "version": "0.0.92",
  "description": "",
  "type": "module",
  "scripts": {
    "check-types": "tsc --noEmit",
    "build": "tsup"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@batijs/tsup": "workspace:*",
    "@types/node": "^16.18.40",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "tsup": "^7.2.0",
    "vite": "^4.5.0",
    "vike": "^0.4.144"
  },
  "dependencies": {
    "@batijs/core": "workspace:*"
  },
  "files": [
    "dist/"
  ],
  "bati": {
    "if": {
      "flag": "tailwindcss"
    }
  },
  "exports": {
    "./postcss.config": {
      "types": "./dist/types/postcss.config.d.ts"
    },
    "./tailwind.config": {
      "types": "./dist/types/tailwind.config.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "postcss.config": [
        "./dist/types/postcss.config.d.ts"
      ],
      "tailwind.config": [
        "./dist/types/tailwind.config.d.ts"
      ]
    }
  }
}